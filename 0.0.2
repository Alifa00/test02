#include <iostream>
#include <sstream>
using namespace std;
void rvrs(int * & ar, unsigned int n) {
    for (unsigned int j = 0; j < n / 2; j++) {
        swap(ar[j], ar[n - j - 1]);
    }
}
void polrvrs(int * & ar, unsigned int pos1, unsigned int n) {
    for (unsigned int j = 0; j < pos1 / 2; j++) {
        swap(ar[j], ar[pos1 - j - 1]);
    }
    for (unsigned int i = 0; i < (n - pos1) / 2; i++) {
        swap(ar[pos1 + i], ar[n - i - 1]);
    }
}
int main() {
    unsigned int n;
    string line;
    getline(cin, line);
    istringstream numstream(line);
    if (!(numstream >> n)) {
        cout << "An error has occuried while reading input data.";
        exit(0);
    }
    getline(cin, line);
    istringstream arrstream(line);
    int * arr = new int[n];
    for (unsigned int i = 0; i < n; i++) {
        if (!(arrstream >> arr[i])) {
            cout << "An error has occuried while reading input data.";
            exit(0);
        }
    }
    int npos;
    getline(cin, line);
    istringstream posstrm(line);
    if (!(posstrm >> npos)) {
        cout << "An error has occuried while reading input data.";
        exit(0);
    }
    polrvrs(arr, n - npos, n);
    rvrs(arr, n);
    for (unsigned int i = 0; i < n; i++) {
        cout << arr[i] << " ";
    }
    return 0;
}
